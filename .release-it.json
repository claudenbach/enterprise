{
  "non-interactive": false,
  "dry-run": true,
  "verbose": false,
  "pkgFiles": ["ids-enterprise/package.json"],
  "increment": "patch",
  "buildCommand": "npx run build",
  "src": {
    "commitMessage": "Release v%s",
    "tagAnnotation": "Release v%s",
    "beforeStartCommand": "npm run eslint && npm run functional:local",
    "afterReleaseCommand": "npm run documentation --site=prod"
  },
  "npm": {
    "publishPath": "publish/package.json",
    "tag": "latest"
  },
  "github": {
    "release": true,
    "releaseName": "v%s",
    "tokenRef": "GITHUB_ACCESS_TOKEN"
  }
}
